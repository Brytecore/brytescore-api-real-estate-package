					"data":  {
						"type": "object",
						"properties": {
							"toEmail":   { "type": "string", "format": "email" },
							"listing":   { "$ref": "#/definitions/listing" },
							"fromEmail": { "type": "string", "format": "email" }
						}
					}
				},
				"required": [ "event", "data" ]
			}
		},
		{
