			},
			"required": [ "event", "data" ]
		},
		{
			"title": "deletedSavedSearch",
			"description": "Deleted a saved search",
			"type": "object",
			"properties": {
				"event": { "enum": [ "realestate.deletedSavedSearch" ] },
				"data":  {
