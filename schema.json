					{
						"properties": {
							"event": { "enum": [ "realestate.calculatedMortgagePayment" ] },
							"price": { "type": "number" },
							"downPayment": { "type": "number" },
							"rate": { "type": "number" }
						},
						"required": [ "event", "price", "downPayment", "rate" ]
					}
				]
			}
		},
		{
			"deletedSavedListing": {
