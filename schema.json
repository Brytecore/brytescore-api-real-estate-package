			"savedListing": {
				"description": "Saved a listing",
				"type": "object",
				"properties": {
					"event": { "enum": [ "realestate.savedListing" ] },
					"data":  {
						"type": "object",
						"properties": {
							"listing": { "$ref": "#/definitions/listing" }
						}
					}
				},
				"required": [ "event", "data" ]
			}
