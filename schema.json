				"bedroomsMin": { "type": "number" },
				"bedroomsMax": { "type": "number" },
				"cities": { "type": [ "array", "null" ], "items": { "type": "string" } },
				"counties": { "type": [ "array", "null" ], "items": { "type": "string" } },
				"description": { "type": [ "string", "null" ] },
				"features": { "type": [ "array", "null" ], "items": { "type": "string" } },
